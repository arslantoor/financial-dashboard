{
  "name": "financial-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@iconify/react": "^5.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "chart.js": "^4.3.0",
    "concurrently": "^9.1.2",
    "d3": "^7.8.5",
    "json-server": "^0.17.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "react-slick": "^0.30.3",
    "react-toastify": "^11.0.5",
    "slick-carousel": "^1.8.1",
    "web-vitals": "^2.1.4",
    "prettier": "^3.5.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "node server.js",
    "vercel-build": "vite build",
    "preview": "vite preview",
    "server": "json-server --watch db.json --port 3001",
    "dev:all": "concurrently \"yarn dev\" \"yarn server\"",
    "format": "prettier --write \"*/.{js,jsx}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9"
  },
  "engines": {
    "node": ">=14.0.0"
  }
